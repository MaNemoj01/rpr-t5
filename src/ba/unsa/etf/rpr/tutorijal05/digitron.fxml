<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="5" prefHeight="250" prefWidth="180" vgap="5" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ba.unsa.etf.rpr.tutorijal05.Controller">
    <stylesheets>
        <URL value="@digitron.css" />
    </stylesheets>
    <columnConstraints>
      <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="25.0" />
      <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="25.0" />
      <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="25.0" />
      <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="25.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints vgrow="ALWAYS" />
      <RowConstraints vgrow="ALWAYS" />
      <RowConstraints vgrow="ALWAYS" />
      <RowConstraints vgrow="ALWAYS" />
      <RowConstraints vgrow="ALWAYS" />
      <RowConstraints vgrow="ALWAYS" />
   </rowConstraints>
   <children>
      <Button maxWidth="Infinity" mnemonicParsing="false" onAction="#btnBrojClick" text="1" GridPane.columnIndex="0" GridPane.rowIndex="4">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></Button>
      <Button maxWidth="Infinity" mnemonicParsing="false" onAction="#btnBrojClick" text="2" GridPane.columnIndex="1" GridPane.rowIndex="4">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></Button>
      <Button maxWidth="Infinity" mnemonicParsing="false" onAction="#btnBrojClick" prefHeight="0.0" text="3" GridPane.columnIndex="2" GridPane.rowIndex="4">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></Button>
      <Button maxWidth="Infinity" mnemonicParsing="false" onAction="#btnBrojClick" text="4" GridPane.columnIndex="0" GridPane.rowIndex="3">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></Button>
      <Button maxWidth="Infinity" mnemonicParsing="false" onAction="#btnBrojClick" text="7" GridPane.columnIndex="0" GridPane.rowIndex="2">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></Button>
      <Button maxWidth="Infinity" mnemonicParsing="false" onAction="#btnBrojClick" text="8" GridPane.columnIndex="1" GridPane.rowIndex="2">
