            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></Button>
      <Button maxWidth="Infinity" mnemonicParsing="false" onAction="#btnBrojClick" text="5" GridPane.columnIndex="1" GridPane.rowIndex="3">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></Button>
      <Button maxWidth="Infinity" mnemonicParsing="false" onAction="#btnBrojClick" prefHeight="27.0" prefWidth="0.0" text="9" GridPane.columnIndex="2" GridPane.rowIndex="2">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></Button>
      <Button maxWidth="Infinity" mnemonicParsing="false" onAction="#btnBrojClick" prefHeight="27.0" prefWidth="37.0" text="6" GridPane.columnIndex="2" GridPane.rowIndex="3">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></Button>
      <Button maxHeight="Infinity" maxWidth="Infinity" onAction="#btnOperacijaClick" text="+" GridPane.columnIndex="3" GridPane.rowIndex="2" GridPane.rowSpan="2">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></Button>
      <Button maxWidth="Infinity" onAction="#btnOperacijaClick" text="\%" GridPane.rowIndex="1" GridPane.valignment="CENTER">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></Button>
      <Button maxWidth="Infinity" onAction="#btnOperacijaClick" text="/" GridPane.columnIndex="1" GridPane.rowIndex="1">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></Button>
      <Button maxWidth="Infinity" onAction="#btnOperacijaClick" text="x" GridPane.columnIndex="2" GridPane.rowIndex="1">
