            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></Button>
      <Button maxWidth="Infinity" onAction="#btnBrojClick" text="." GridPane.columnIndex="2" GridPane.rowIndex="5">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></Button>
      <Button maxHeight="Infinity" maxWidth="Infinity" onAction="#btnEnterClick" text="=" GridPane.columnIndex="3" GridPane.rowIndex="4" GridPane.rowSpan="2">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></Button>
      <TextField fx:id="Polje" prefHeight="27.0" prefWidth="0.0" styleClass="textFieldStil" text="${controller.username}" GridPane.columnIndex="0" GridPane.columnSpan="4">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></TextField>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</GridPane>
